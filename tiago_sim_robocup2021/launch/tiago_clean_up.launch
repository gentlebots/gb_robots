<?xml version="1.0" encoding="UTF-8"?>
<launch>

  <arg name="world_name" default="$(find tiago_sim_robocup2021)/worlds/wrs2020_clean_up_demo.world" />
  <!-- Load Gazebo -->
  <include file="$(find tiago_sim_robocup2021)/launch/robocup2021_gazebo.launch" >
    <arg name="world_name" value="$(arg world_name)"/>
    <arg name="clean_up" value="true"/>
  </include>
  <!-- Spawn TIAGo -->
  <include file="$(find tiago_sim_robocup2021)/launch/tiago_spawn.launch" ></include>
  <!-- TIAGo controllers -->
  <include file="$(find tiago_sim_robocup2021)/launch/tiago_sim_controllers.launch" ></include>

  <!-- Allowed joint-value tolerance for validation that trajectory's first point matches current robot state -->
  <param name="/move_group/trajectory_execution/allowed_start_tolerance" value="0.0"/>
  <param name="/move_group/octomap_resolution" value="0.025"/>
  <!--<param name="/move_group/constraint_approximations_path" value="$(find gb_tiago_manipulation_demo)/config/constraint_db/"/>-->

</launch>
